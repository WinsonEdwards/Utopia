println("Starting cross-language test")

@lang python {
    def add(a, b):
        return a + b
    
    def greet(name):
        return f"Hello, {name}!"
}

@lang javascript {
    function multiply(x, y) {
        return x * y;
    }
}

let result = python::add(5, 3)
let greeting = python::greet("World")
let product = javascript::multiply(4, 7)

println("Results:")
println(result)
println(greeting)
println(product) 