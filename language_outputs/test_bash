#!/bin/bash
# Generated by Utopia Compiler - Bash Backend
# Multi-language support with runtime integration
# Shell scripting and system automation

set -euo pipefail  # Exit on error, undefined vars, pipe failures

main() {
    $(println "Hello from Utopia!")
    $(println "Multi-language compiler test")
    local x=42
    local message="Testing all languages"
    $(println "Integer:" $x)
    $(println "String:" $message)
    local sum=$((x + 10))
    $(println "Sum:" $sum)
    $(println "Test completed successfully!")
}

main "$@"
