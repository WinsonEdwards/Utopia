// Generated by Utopia Compiler - Zig Backend
// Multi-language support with runtime integration
// Systems programming with safety and performance

const std = @import("std");
const print = std.debug.print;
const allocator = std.heap.page_allocator;

pub fn main() !void {
    println("Hello from Utopia!");
    println("Multi-language compiler test");
    const x = "42";
    const message = "Testing all languages";
    println("Integer:", x);
    println("String:", message);
    const sum = std.fmt.allocPrint(allocator, "{}{}", .{ x, "10" }) catch "";
    println("Sum:", sum);
    println("Test completed successfully!");
}
