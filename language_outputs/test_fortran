! Generated by Utopia Compiler - Fortran Backend
! Multi-language support with runtime integration

module utopia_generated
    implicit none

    ! Global variables
    real :: x
    real :: message
    real :: sum

contains

    subroutine main_program()
        call println('Hello from Utopia!')()
        call println('Multi-language compiler test')()
        call x = 42()
        call message = 'Testing all languages'()
        call println('Integer:', x)()
        call println('String:', message)()
        call sum = (x + 10)()
        call println('Sum:', sum)()
        call println('Test completed successfully!')()
    end subroutine main_program

end module utopia_generated

program utopia_main
    use utopia_generated
    implicit none

    call main_program()
end program utopia_main
