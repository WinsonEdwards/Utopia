// Test MATLAB compilation
// Demonstrates MATLAB functions for engineering and numerical computing

@lang matlab {
function solveQuadratic(a, b, c) {
    return (-b + sqrt(b * b - 4 * a * c)) / (2 * a);
}

function calculateMatrix(rows, cols) {
    return ones(rows, cols);
}

function performFFT(signal) {
    return fft(signal);
}
}

@lang main {
let coeff_a = 1;
let coeff_b = -5;
let coeff_c = 6;
let root = matlab::solveQuadratic(coeff_a, coeff_b, coeff_c);

let matrix_rows = 3;
let matrix_cols = 4;
let matrix_result = matlab::calculateMatrix(matrix_rows, matrix_cols);

let input_signal = [1, 2, 3, 4, 5];
let fft_result = matlab::performFFT(input_signal);

console.log("Quadratic root: " + root);
console.log("Matrix dimensions: " + matrix_rows + "x" + matrix_cols);
console.log("FFT computed successfully");
} 