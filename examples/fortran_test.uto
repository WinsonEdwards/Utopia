// Test Fortran compilation
// Demonstrates Fortran code generation for scientific computing and HPC

@lang fortran {
function calculateDistance(x1, y1, x2, y2) {
    return ((x2 - x1) * (x2 - x1) + (y2 - y1) * (y2 - y1));
}

function computeAverage(a, b, c) {
    return (a + b + c) / 3.0;
}
}

@lang main {
let point1X = 1.0;
let point1Y = 2.0;
let point2X = 4.0;
let point2Y = 6.0;
let distance = fortran::calculateDistance(point1X, point1Y, point2X, point2Y);

let value1 = 10.5;
let value2 = 15.3;
let value3 = 12.8;
let average = fortran::computeAverage(value1, value2, value3);

console.log("Distance squared: " + distance);
console.log("Average: " + average);
} 