// Test Visual Basic .NET compilation
// Demonstrates basic VB.NET code generation

@lang visualbasic {
function calculate(x, y) {
    return x * y + 10;
}

function formatMessage(name, value) {
    return "Hello " + name + ", your result is: " + value;
}
}

@lang main {
let userName = "VB Developer";
let result = vb::calculate(5, 8);
let message = vb::formatMessage(userName, result);

console.log(message);
} 